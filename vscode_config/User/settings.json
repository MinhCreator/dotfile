{
    // WorkBench and workspace configure
    "workbench.sideBar.location": "right",
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "off",
    "workbench.reduceMotion": "on",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.tree.indent": 15,
    "workbench.editor.defaultBinaryEditor": "default",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.productIconTheme": "fluent-icons",
    "workbench.colorTheme": "One Dark Pro Flat",
    "workbench.colorCustomizations": {},
    "workbench.statusBar.visible": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.activityBar.location": "top",
    // file configure    
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/.": true,
        "**/.git": false,
        "**/*.import": true,
        "**/*.svg": true,
        "**/*.tscn": true
    },
    // editor configure    
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrainsMono Nerd Font", //"CaskaydiaCove Nerd Font",//"FiraCode Nerd Font", // //"'CaskaydiaCove Nerd Font'",
    //"editor.fontWeight": "500",
    "editor.codeLens": false,
    "editor.codeActionWidget.showHeaders": false,
    "editor.minimap.renderCharacters": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.tabCompletion": "on",
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "expand",
    "editor.matchBrackets": "near",
    "editor.scrollbar.horizontalScrollbarSize": 1.5,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.guides.bracketPairs": "active",
    "editor.accessibilitySupport": "off",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.useTabStops": false,
    "editor.minimap.maxColumn": 100,
    "editor.colorDecoratorsLimit": 350,
    "editor.bracketPairColorization.enabled": false,
    "editor.inlineCompletionsAccessibilityVerbose": true,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 6,
    "editor.minimap.enabled": false,
    "editor.inlayHints.enabled": "off",
    "editor.experimentalWhitespaceRendering": "font",
    "editor.renderFinalNewline": "off",
    // Window configure 
    "window.density.editorTabHeight": "compact",
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "toggle",
    "window.autoDetectHighContrast": false,
    // Extension configure
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.startupPrompt": "never",
    "markdown.updateLinksOnFileMove.enabled": "always",
    "outline.icons": false,
    // Explorer configure
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    // Terminal configure
    "terminal.integrated.fontWeight": "bold",
    "terminal.integrated.tabs.enableAnimation": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.tabs.defaultIcon": "console",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font'", //"'CaskaydiaCove Nerd Font'",//"'MesloLGM Nerd Font'",
    "terminal.integrated.tabs.defaultColor": "terminal.ansiGreen",
    "debug.enableStatusBarColor": false,
    "debug.showInStatusBar": "never",
    "debug.terminal.clearBeforeReusing": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    // Screencast Mode configure
    "screencastMode.fontSize": 20,
    "screencastMode.keyboardOverlayTimeout": 500,
    "screencastMode.mouseIndicatorColor": "#00FFFFFF",
    "errorLens.gutterIconSet": "squareRounded",
    "errorLens.gutterIconSize": "auto",
    "errorLens.errorGutterIconColor": "#f02929",
    "errorLens.warningGutterIconColor": "#ff840f",
    "errorLens.codeLensOnClick": "searchForProblem",
    "errorLens.enabledDiagnosticLevels": [
        "error"
    ],
    "errorLens.followCursor": "closestProblem",
    "errorLens.fontStyleItalic": true,
    "errorLens.fontWeight": "bold",
    "errorLens.statusBarIconsUseBackground": false,
    // HTML configure
    "html.format.indentInnerHtml": true,
    // Notebook configure
    "audioCues.notebookCellCompleted": "off",
    "audioCues.notebookCellFailed": "off",
    "notebook.breadcrumbs.showCodeCells": false,
    "notebook.globalToolbar": false,
    "notebook.globalToolbarShowLabel": "never",
    "notebook.insertToolbarLocation": "hidden",
    "diffEditor.experimental.showEmptyDecorations": false,
    "diffEditor.maxComputationTime": 7000,
    // Search configure
    "search.quickOpen.includeHistory": false,
    "search.decorations.colors": false,
    "search.searchOnType": false,
    // Breadcrumbs configure
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "breadcrumbs.filePath": "off",
    "breadcrumbs.showArrays": false,
    "breadcrumbs.showBooleans": false,
    "breadcrumbs.showClasses": false,
    "breadcrumbs.showConstants": false,
    "breadcrumbs.showConstructors": false,
    "breadcrumbs.showEnumMembers": false,
    "breadcrumbs.showEnums": false,
    "breadcrumbs.showFields": false,
    "breadcrumbs.showEvents": false,
    "breadcrumbs.showFiles": false,
    "breadcrumbs.showFunctions": false,
    "breadcrumbs.showInterfaces": false,
    "breadcrumbs.showKeys": false,
    "breadcrumbs.showMethods": false,
    "breadcrumbs.showModules": false,
    "breadcrumbs.showNamespaces": false,
    "breadcrumbs.showNull": false,
    "breadcrumbs.showNumbers": false,
    "breadcrumbs.showObjects": false,
    "breadcrumbs.showOperators": false,
    "breadcrumbs.showPackages": false,
    "breadcrumbs.showProperties": false,
    "breadcrumbs.showStrings": false,
    "breadcrumbs.showStructs": false,
    "breadcrumbs.showTypeParameters": false,
    "breadcrumbs.showVariables": false,
    "breadcrumbs.symbolPath": "off",
    // Codeium configure
    "codeium.enableCodeLens": false,
    // Python configure
    "python.languageServer": "Pylance",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.addImport.exactMatchOnly": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.enablePytestSupport": false,
    // Godot configure
    "godotTools.lsp.serverPort": 6005,
    "godotTools.inlayHints.gdscript": true,
    "focus.highlightRange": "indent",
    "focus.opacity": "0.9",
    "focus.highlightLines": 1000,
    "[json]": {
        // "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "color-highlight.markerType": "foreground",
    // custom vscode css
    "vscode_custom_css.imports": [
        "file:///C:/Users/Administrator/.config/vscode_config/vscode_custom.css",
        "file:///C:/Users/Administrator/.config/vscode_config/vscode_script.js",
    ],
    "workbench.welcomePage.walkthroughs.openOnInstall": false,
    "workbench.tips.enabled": false,
    "errorLens.delay": 500,
    // "window.commandCenter": false,
}